@model IEnumerable<Rkna_Project.Models.Car_Specifications_Table>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.Label("Care Model")
        </th>
        <th>
            @Html.Label("Car plate Number")
        </th>
        <th>
            @Html.Label("Car Owner")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Care_Model)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Car_plate_Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUser.Email)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Car_Spe_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Car_Spe_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Car_Spe_ID })
        </td>
    </tr>
}

</table>
