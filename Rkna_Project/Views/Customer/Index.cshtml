@model IEnumerable<Rkna_Project.Models.Customer_Slut_Table>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table  w-auto small">
    <tr>
        <th>
            @Html.DisplayName("Slut Date")
        </th>
        <th>
            @Html.DisplayName("Slut Start Time")
        </th>
        <th>
            @Html.DisplayName("Slut End Time")
        </th>
        <th>
            @Html.DisplayName("Cheeck Code")
        </th>
        <th>
            @Html.DisplayName("User Email")
        </th>
       
        <th>
            @Html.DisplayName("Slut Name")
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Cus_Slut_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cus_Slut_S_Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cus_Slut_E_Time)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cheeck_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AspNetUser.Email)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Slut_Table.Name)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Customer_Slut_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Customer_Slut_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Customer_Slut_ID })
        </td>
    </tr>
}

</table>
